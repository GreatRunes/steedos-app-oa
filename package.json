{
    "name": "steedos-project-oa",
    "version": "1.0.0",
    "description": "",
    "engines": {
        "node": ">=10"
    },
    "main": "server.js",
    "scripts": {
        "debug": "nodemon server.js",
        "start": "node --max-old-space-size=2048 --stack_trace_limit=200 server.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "os": "cp -r ../object-server/packages/* node_modules/@steedos/",
        "oss": "cp -r ../object-server/packages/standard-objects/* node_modules/@steedos/standard-objects/ && yarn start",
        "pkg": "pkg . --options max_old_space_size=2048 --out-path=dist/"
    },
    "bin": "./server.js",
    "pkg": {
        "scripts": [],
        "assets": [
            "node_modules/@steedos/schemas/object/schema.json",
            "node_modules/@steedos/standard-objects/**",
            "node_modules/@steedos/webapp/build/**",
            "node_modules/@steedos/i18n/**",
            "node_modules/@steedos/app-crm/**",
            "node_modules/@steedos/steedos-plugin-workflow/**",
            "steedos-config.yml",
            ".env"
        ],
        "targets": [
            "node12-macos-x64",
            "node12-win-x64"
        ]
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@steedos/accounts": "1.22.4",
        "@steedos/app-contracts": "1.22.4",
        "@steedos/app-crm": "1.22.4",
        "@steedos/plugin-enterprise": "1.22.4",
        "@steedos/plugin-qywx": "^0.0.10",
        "@steedos/steedos-plugin-workflow": "^2.1.1",
        "aliyun-sdk": "1.12.3",
        "aws-sdk": "2.0.23",
        "dotenv-flow": "^3.1.0",
        "node-rsa": "^1.0.8",
        "pug": "^3.0.0",
        "request-ip": "^2.1.3",
        "steedos-server": "^1.22.4"
    },
    "devDependencies": {
        "nodemon": "^1.19.1"
    },
    "homepage": "https://developer.steedos.com",
    "resolutions": {
        "**/lodash": "^4.17.15",
        "**/handlebars": "^4.3.0",
        "**/debug": "^3.1.0",
        "**/hoek": "^5.0.3",
        "**/cryptiles": "^4.1.2",
        "**/bcrypt": "3.0.6",
        "**/samlp": "3.4.1",
        "**/xmldom": "0.2.1",
        "**/fibers": "^4.0.3"
    }
}
